    "deploy:testnet": "hardhat run scripts/deploy.js --network baseSepolia",
    "deploy:battle": "hardhat run scripts/deploy-battle-contract.js --network base",
    "deploy:battle:testnet": "hardhat run scripts/deploy-battle-contract.js --network baseSepolia",
    "verify": "hardhat verify --network base",
    "test": "hardhat test",
    "test:agents": "npx tsx scripts/test-agentic-system.ts",
    "test:agents-fast": "npx tsx scripts/test-agents-fast.ts",
    "test:agents-mock": "npx tsx scripts/test-agents-mock.ts",
    "test:serper": "npx tsx scripts/test-serper-api.ts",
    "test:battle": "npx tsx scripts/test-battle-generation.js",
    "test:news": "npx tsx scripts/test-news-api.js",
    "test:politics-crypto": "npx tsx scripts/test-politics-crypto-news.js",
    "test:gemini": "npx tsx scripts/test-gemini-ai.js",
    "test:vercel-ai": "npx tsx scripts/test-vercel-ai.js",
    "test:ai-similarity": "npx tsx scripts/test-ai-similarity.js",
    "test:similarity-integration": "npx tsx scripts/test-similarity-integration.js",
    "test:retry-strategy": "npx tsx scripts/test-retry-strategy.js",
    "test:cache-key": "npx tsx scripts/test-cache-key.js",
    "test:optimized-judging": "npx tsx scripts/test-optimized-judging.js",
    "force:battle-transition": "npx tsx scripts/force-battle-transition.js",
    "db:setup": "./setup-local-dev.sh",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:logs": "docker-compose logs postgres",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.14",
