    "ai": "^5.0.47",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0",
    "hardhat": "^3.0.6",
    "next": "15.3.4",
    "pg": "^8.16.3",
    "prisma": "^6.16.2",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
