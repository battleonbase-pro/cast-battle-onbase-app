{
  "name": "newscast-battle-onbase",
  "version": "1.0.0",
  "description": "AI-Powered Debate Battles on Base Network with Farcaster Frames",
  "private": true,
  "type": "module",
  "keywords": [
    "farcaster",
    "frame",
    "battle",
    "token",
    "base",
    "news",
    "trending",
    "ai",
    "onchainkit"
  ],
  "author": "NewsCast Debate Team",
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "setup": "npx tsx scripts/setup-env.js",
    "setup:env": "npx tsx scripts/setup-env.js",
    "setup:gemini": "npx tsx scripts/setup-gemini-env.js",
    "setup:onchainkit": "npx tsx scripts/setup-onchainkit.js",
