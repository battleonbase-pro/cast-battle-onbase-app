# Base Sepolia Environment Variables
# Copy this file to .env and fill in your values

# Private key of the deployer wallet (keep this secure!)
PRIVATE_KEY=your_private_key_here

# Oracle address (your backend wallet that will sign winner results)
ORACLE_ADDRESS=0x0000000000000000000000000000000000000000

# Basescan API key for contract verification (optional)
BASESCAN_API_KEY=your_basescan_api_key_here

# RPC URLs (these are public, no need to change)
BASE_SEPOLIA_RPC=https://sepolia.base.org
BASE_MAINNET_RPC=https://mainnet.base.org
